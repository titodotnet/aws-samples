<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>
  <appSettings>
    <add key="AwsAccessKeyId" value=""/>
    <add key="AwsSecretKey" value="" />
    <add key="AwsS3BucketName" value="" />
    <add key="TargetFileName" value="" />    <!--Name of the file to be copied into.-->
    <add key="StorageAccount" value="DefaultEndpointsProtocol=https;AccountName={your account name here};AccountKey={your account key here};EndpointSuffix=core.windows.net"/>
    <add key="ContainerName" value="" />    <!--Your container name-->
    <add key="BlobFileName" value=""/>    <!--Your BLOB file name to be copied (Source file)-->
  </appSettings>
</configuration>